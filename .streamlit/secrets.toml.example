# Example secrets file for Streamlit Cloud
# Copy this to Streamlit Cloud dashboard under Settings > Secrets
# DO NOT commit the actual secrets.toml file to GitHub

# USDA NASS API Key (optional)
# Get from: https://quickstats.nass.usda.gov/api
NASS_API_KEY = "YOUR_NASS_API_KEY_HERE"

# CIMIS API Key (optional)
# Get from: http://et.water.ca.gov/Rest/Index
CIMIS_APP_KEY = "YOUR_CIMIS_APP_KEY_HERE"

# Data directory (usually not needed, defaults to ./data)
# DATA_DIR = "./data"
